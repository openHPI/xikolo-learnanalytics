# Ignore all logfiles and tempfiles.
/log/*.log
/tmp
/client/pkg

# IntellJ / RubyMine
/.idea
/.idea/
*.xml
*.iml
/.generators
/.rakeTasks

# Dev files
.rvmrc
.rbenv
.rspec
spec/examples.txt

# ignore autogenerated test coverage data
/coverage/*

# Local config
/config/xikolo.development.yml

# Integration tests
/config/initializers/integration*
/config/services.integration.yml
config/xikolo.integration.yml
